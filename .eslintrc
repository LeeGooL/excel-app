{
	"parser": "babel-eslint",
	"env": {
		"es6": true,
		"browser": true,
		"node": true
	},
	"rules": {
		"arrow-parents": "off",
		"comma-dangle": "off",
		"require-jsdoc": "off",
		"no-tabs": "off",
		"indent": ["error", "tab"],
		"object-curly-spacing": "off",
		"comma": "off"
	},
	"extends": ["eslint:recommended", "google"]
}
