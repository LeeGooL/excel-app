{
	"parser": "babel-eslint",
	"env": {
		"es6": true,
		"browser": true,
		"node": true
	},
	"rules": {
		"arrow-parents": "off",
		"comma-dangle": "off",
		"require-jsdoc": "off",
		"no-tabs": "off",
		"indent": ["error", "tab"],
		"object-curly-spacing": "off",
		"comma": "off",
		"linebreak-style": ["off", "windows"],
		"no-trailing-spaces": "off",
		"quotes": ["error", "single"],
		"operator-linebreak": "off",
		"no-unused-vars": "warn",
		"max-len": ["error", 100]
	},
	"extends": ["eslint:recommended", "google"]
}
